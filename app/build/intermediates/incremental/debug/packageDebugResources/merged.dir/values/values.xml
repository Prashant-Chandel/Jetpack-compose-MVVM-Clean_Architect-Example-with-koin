<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#FF000000</color>
    <color name="black_overlay">#66000000</color>
    <color name="colorPrimary">#FF8300</color>
    <color name="colorPrimaryDark">#FF7300</color>
    <color name="favourite">#ffE91E63</color>
    <color name="light_blue_600">#FF039BE5</color>
    <color name="light_blue_900">#FF01579B</color>
    <color name="light_blue_A200">#FF40C4FF</color>
    <color name="light_blue_A400">#FF00B0FF</color>
    <color name="white">#FFFFFFFF</color>
    <dimen name="card_view_flat_elevation">2dp</dimen>
    <dimen name="card_view_radius">10dp</dimen>
    <dimen name="margin_medium">20dp</dimen>
    <dimen name="margin_small">10dp</dimen>
    <dimen name="margin_xSmall">5dp</dimen>
    <string name="app_name">MVVMCleanExample</string>
    <string name="both_are_not_equal">Both are not equal</string>
    <string name="cat_image">Cat Image</string>
    <string name="cats_are_loaded">Cats are loaded.</string>
    <string name="favorite_screen_empty_list_text">You haven\'t added\nany cat to your favorites yet</string>
    <string name="favourite_icon">Favourite icon</string>
    <string name="home">Home</string>
    <string name="home_screen">Home Screen</string>
    <string name="img">Image full view\n</string>
    <string name="my_favorites">My Favourites</string>
    <string name="my_favourite">MyFavourite</string>
    <string name="no_internet_connection">No internet connection</string>
    <string name="you_can_check_your_call_favourite_cats">"You can check your call Favourite cats. "</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
         
         <item name="colorPrimary">@color/colorPrimary</item>
         <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
         <item name="colorAccent">@color/colorPrimary</item>

         
         <item name="android:windowActionBar">false</item>
         <item name="android:windowNoTitle">true</item>

         
        <item name="android:statusBarColor">@color/colorPrimaryDark</item>
        
        <item name="android:windowActivityTransitions">true</item>

        
        <item name="android:windowEnterTransition">@transition/fade</item>
        <item name="android:windowExitTransition">@transition/fade</item>
        
        <item name="android:textColorPrimary">@color/colorPrimary</item>
        <item name="android:textColorSecondary">@color/white</item>

        
        <item name="toolbarStyle">@style/Widget.AppCompat.Toolbar</item>
    </style>
    <style name="Base.Theme.app" parent="Theme.Material3.DayNight.NoActionBar">
        
        
    </style>
    <style name="Theme.base" parent="Base.Theme.app"/>
    <style name="Theme.base.Fullscreen" parent="Theme.base">
        <item name="android:actionBarStyle">@style/Widget.Theme.LBG.ActionBar.Fullscreen</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="ThemeOverlay.base.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_600</item>
        <item name="fullscreenTextColor">@color/light_blue_A200</item>
    </style>
    <style name="Widget.Theme.LBG.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>
    <style name="Widget.Theme.LBG.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>
    <style name="low_elevation_card" parent="CardView">
        <item name="cardCornerRadius">@dimen/card_view_radius</item>
        <item name="cardElevation">@dimen/card_view_flat_elevation</item>
    </style>
    <declare-styleable name="FullscreenAttrs">
        <attr format="color" name="fullscreenBackgroundColor"/>
        <attr format="color" name="fullscreenTextColor"/>
    </declare-styleable>
</resources>